# This file is managed by a scripted process.
# Manual changes will be destroyed if the script is re-applied.

[local-google-chrome]
name=local-google-chrome
baseurl={{ google_chrome__yum_baseurl + "$basearch" }}
{% if google_chrome__yum_proxy is defined %}
proxy={{ google_chrome__yum_proxy }}
{% endif %}
enabled=1
gpgcheck=1
gpgkey=file:///ansible-managed/google-chrome/linux_signing_key.pub
